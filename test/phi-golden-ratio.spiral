class GoldenSpiral {
  constructor(phi) {
    this.ratio = phi || φ;
    this.spiral = [];
  }
  
  generate(iterations = 13) {
    for (let i = 0; i < iterations; i++) {
      this.spiral.push(this.fibonacci(i) * this.ratio);
    }
    return this.spiral;
  }
  
  fibonacci(n) {
    if (n <= 1) return n;
    return this.fibonacci(n - 1) + this.fibonacci(n - 2);
  }
}

